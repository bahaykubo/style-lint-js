{
  "name": "style-lint-js",
  "description": "Lint and style checker using ESLint and Prettier with default rules that can be extended.",
  "version": "1.1.1",
  "main": "init.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bahaykubo/style-lint-js.git"
  },
  "keywords": [
    "eslint",
    "prettier",
    "lint",
    "style"
  ],
  "bin": {
    "style-lint-js": "init.js"
  },
  "files": [
    "init.js",
    ".eslintrc.js",
    ".eslintrc-ts.js",
    ".prettierrc.js",
    "defaults/"
  ],
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.0.3"
  },
  "scripts": {
    "ci": "rm -rf node_modules && yarn install --frozen-lockfile"
  }
}
